# Vercel Deployment Environment Variables
# Copy this to your Vercel project environment variables

# Environment
REACT_APP_ENV=production
NODE_ENV=production

# API Configuration - Update with your deployed backend URL
REACT_APP_API_URL=https://your-backend-api-url.com/api
REACT_APP_SOCKET_URL=https://your-backend-api-url.com

# Environment Visual Indicators
REACT_APP_ENV_NAME=Vercel Production
REACT_APP_ENV_COLOR=#EF4444
REACT_APP_ENV_BADGE=false
REACT_APP_DEBUG_MODE=false

# Social Authentication (Production Keys)
REACT_APP_GOOGLE_CLIENT_ID=your-vercel-google-client-id
REACT_APP_FACEBOOK_APP_ID=your-vercel-facebook-app-id
REACT_APP_GOOGLE_OAUTH_REDIRECT_URI=https://your-vercel-domain.com/auth/google/callback

# Firebase (Production)
REACT_APP_FIREBASE_API_KEY=your-vercel-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-vercel-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-vercel-project-id

# Payment Configuration (Live Keys)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_live_vercel_stripe_key
REACT_APP_RAZORPAY_KEY_ID=rzp_live_vercel_key
REACT_APP_PAYPAL_CLIENT_ID=vercel_paypal_client_id
REACT_APP_GOOGLE_PAY_MERCHANT_ID=vercel_google_pay_merchant_id
REACT_APP_GOOGLE_PAY_ENVIRONMENT=PRODUCTION

# InstaMojo API
INSTAMOJO_API_KEY=your-vercel-instamojo-key
INSTAMOJO_AUTH_TOKEN=your-vercel-instamojo-token
INSTAMOJO_URL=https://www.instamojo.com/api/1.1/

# Analytics Configuration (Production)
REACT_APP_GA_MEASUREMENT_ID=G-VERCEL-TRACKING
REACT_APP_FB_PIXEL_ID=vercel_pixel_id

# Production Features
REACT_APP_ENABLE_REDUX_DEVTOOLS=false
REACT_APP_ENABLE_CONSOLE_LOGS=false
REACT_APP_MOCK_PAYMENTS=false